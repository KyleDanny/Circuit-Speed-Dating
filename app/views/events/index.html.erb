<h2> Sessions: </h2>

<%= month_calendar(events: @events, end_attribute: :end_time) do |date, events| %>
  <%= date %>

  <% events.each do |event| %>
    <div class="meeting">
      <p><%= link_to event.name, event_lobbies_path(event) %></p>
      <p class="text-muted"> <%= event.time %> </p>
      <p> <%= link_to "Book", new_event_booking_path(event) %> </p>
    </div>
  <% end %>
<% end %>
