<div class="col-6 card home-page-pages">
  <div class="image-container">
    <img class="home-picture" src="<%= asset_path('logo.jpg') %>"/>
  </div>
  <div class="image-container">
    <img class="home-picture" src="<%= asset_path('home-page-image.jpg') %>"  alt="couple-dating"/>
  </div>
  <div class="row justify-content-center">
    <% if current_user %>
    <p> <%= link_to "My Profile", profile_path(current_user), class: "btn rounded-pill border" %> </p>
    <% end %>
    <p> <%= link_to "Browse Profiles", profiles_path, class: "btn rounded-pill border" %> </p>
  </div>
</div>
