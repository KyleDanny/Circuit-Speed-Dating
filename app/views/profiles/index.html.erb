<div class="container">

  <% if current_user %>
    <p> <%= link_to "Back", profile_path(current_user) %> </p>
  <% end %>

  <p> <%= link_to "All Dating Sessions", events_path %> </p>

  <h1><strong> Find your Match </strong></h1>

  <% @users.each do |user| %>

    <% if user.photos.attached? %>
      <% user.photos.each do |photo| %>
      <%= cl_image_tag photo.key, height: 300 , width: 200, :crop=>"fill", :radius => 20 %>
      <% end %>
    <% end %>

    <p> <%= link_to user.name, profile_path(user) %> </p>
    <p> <%= user.age %> </p>
    <p> <%= user.gender %> </p>
    <p> <%= user.height %> </p>
    <p> <%= user.address %> </p>

  <% end %>
</div>
